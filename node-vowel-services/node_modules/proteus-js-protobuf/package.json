{
  "name": "proteus-js-protobuf",
  "repository": "https://github.com/netifi-proteus/proteus-js-protobuf",
  "version": "0.0.4",
  "description": "Proteus JavaScript Codegen Plugin for Protobuf Compiler",
  "contributors": [
    "Ryland Degnan <ryland@netifi.com>"
  ],
  "bugs": {
    "url": "https://github.com/netifi-proteus/proteus-js-protobuf/issues"
  },
  "keywords": [
    "rsocket",
    "proteus",
    "protobuf"
  ],
  "bin": {
    "proteus_js_protoc_plugin": "./protoc_plugin.js"
  },
  "files": [
    "index.js",
    "protoc_plugin.js",
    "LICENSE"
  ],
  "main": "index.js",
  "scripts": {
    "install": "prebuild-install || cmake-js rebuild",
    "rebuild": "prebuild --backend cmake-js --include-regex .* --compile",
    "prebuild": "prebuild --backend cmake-js --include-regex .* --all --strip --verbose",
    "protoc": "npm run install && protoc --proto_path=proto --js_out=import_style=commonjs,binary:build --proteus_out=build --plugin=protoc-gen-proteus=build/Release/proteus_js_plugin proto/proteus/*.proto"
  },
  "dependencies": {
    "prebuild-install": "^4.0.0",
    "cmake-js": "^3.6.2"
  },
  "devDependencies": {
    "prebuild": "^7.6.0",
    "prebuild-ci": "^2.2.3"
  }
}
